embed-server --server-config=standalone-full-ha.xml
/subsystem=datasources/jdbc-driver=mysql:add(driver-name=mysql, driver-module-name=com.mysql.jdbc, xa-datasource-class=com.mysql.jdbc.jdbc2.optional.MysqlXADataSource)
data-source add --name=famifarm-api --jndi-name=java:jboss/datasources/famifarm-api --enabled=true --jta=true --use-ccm=true --connection-url=jdbc:mysql://${env.FAMIFARM_API_DB_HOST}:${env.FAMIFARM_API_DB_PORT}/${env.FAMIFARM_API_DB_NAME}${env.FAMIFARM_API_DB_OPTS} --driver-name=mysql --user-name=${env.FAMIFARM_API_DB_USERNAME} --password=${env.FAMIFARM_API_DB_PASSWORD} --check-valid-connection-sql="SELECT 1" --validate-on-match=false --share-prepared-statements=false --max-pool-size=1000
/subsystem=transactions:write-attribute(name=node-identifier,value=${env.FAMIFARM_TX_IDENTIFIER})